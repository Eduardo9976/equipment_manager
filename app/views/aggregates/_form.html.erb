
<%= form_with( model: @aggregate, local:true ) do |f| %>
  <%= f.label :collaborator_id, 'Colaborador'%>
  <%= f.collection_select :collaborator_id, @collaborator, :id, :name%>
  <%= f.label :product_id, 'Produto'%>
  <%= f.collection_select :product_id, @product, :id, :name%>
  <%= f.label :description_id, 'Descrição'%>
  <%= f.collection_select :description_id, @description, :id, :model%>
  <%= f.submit('Enviar') %>
<% end %>